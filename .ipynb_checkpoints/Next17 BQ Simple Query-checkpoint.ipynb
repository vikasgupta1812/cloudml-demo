    "SELECT\n",
    "  title,\n",
    "  SUM(views) AS views\n",
    "FROM\n",
    "  [bigquery-samples:wikipedia_benchmark.Wiki10B]\n",
    "WHERE\n",
    "  REGEXP_MATCH(title, r' Hello ')\n",
    "GROUP BY\n",
